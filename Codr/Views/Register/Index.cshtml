
@{
    ViewData["Title"] = "Register";
}

<form class="content" action="Register/Process" method="post">
    <p>Email <input type="text" name="email" required pattern="\S+" /> </p>
    <p>Password <input type="password" name="password" required pattern="\S+" /> </p>
    <p>First Name <input type="text" name="firstName" required pattern="\S.+" /> </p>
    <p>Last Name  <input type="text" name="lastName" /> </p>
    <input type="submit" value="Register"/>
</form>